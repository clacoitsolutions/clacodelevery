    <style>
        body {
            font-family: Rubik, sans-serif;
            position: relative;
            font-weight: 400;
            font-size: 15px;
        }

        ul {
            padding: 0;
            margin: 0;
        }

        li {
            list-style: none;
        }

        a:focus,
        a:hover {
            text-decoration: none;
            -webkit-transition: .3s ease;
            -o-transition: .3s ease;
            transition: .3s ease;
        }

        a:focus {
            outline: 0;
        }

        img {
            max-width: 100%;
        }

        p {
            font-size: 16px;
            line-height: 30px;
            color: #fff; /* changed text color to white */
            font-weight: 300;
        }

        h4 {
            font-family: Rubik, sans-serif;
            margin: 0;
            font-weight: 400;
            padding: 0;
            color: #fff; /* changed text color to white */
        }

        a {
            color: #fff; /* changed text color to white */
        }

        .no-padding {
            padding: 0 !important;
        }

        .go_top {
            line-height: 40px;
            cursor: pointer;
            width: 40px;
            background: #5867dd;
            color: #fff;
            position: fixed;
            -webkit-box-shadow: 0 4px 4px rgba(0, 0, 0, .1);
            box-shadow: 0 4px 4px rgba(0, 0, 0, .1);
            -webkit-border-radius: 50%;
            border-radius: 50%;
            right: -webkit-calc((100% - 1140px) / 2);
            right: calc((100% - 1140px) / 2);
            z-index: 111;
            bottom: 80px;
            text-align: center;
        }

            .go_top span {
                display: inline-block;
            }

        .footer-big {
            padding: 105px 0 65px 0;
            background-color: #FA2365; /* changed background color to #FA2365 */
        }

        .footer--light {
            background: #FA2365; /* changed background color to #FA2365 */
        }

        .footer-big .footer-widget {
            margin-bottom: 40px;
        }

        .footer-big .footer-menu ul li a,
        .footer-big p,
        .footer-big ul li {
            color: #fff; /* changed text color to white */
            -webkit-transition: color .3s ease; /* added transition for color change */
            -o-transition: color .3s ease;
            transition: color .3s ease;
        }

        .footer-menu {
            padding-left: 48px;
        }

            .footer-menu ul li a {
                font-size: 15px;
                line-height: 32px;
            }

                .footer-menu ul li a:hover {
                    color: #d3d3d3; /* changed color to dark white on hover */
                }

        .footer-menu--1 {
            width: 100%;
        }

        .footer-widget-title {
            line-height: 42px;
            margin-bottom: 10px;
            font-size: 18px;
        }

        .mini-footer {
            background: #192027;
            text-align: center;
            padding: 32px 0;
        }

            .mini-footer p {
                margin: 0;
                line-height: 26px;
                font-size: 15px;
                color: #999;
            }

                .mini-footer p a {
                    color: #5867dd;
                }

                    .mini-footer p a:hover {
                        color: #34bfa3;
                    }

        .widget-about img {
            display: block;
            margin-bottom: 30px;
        }

        .widget-about p {
            font-weight: 400;
        }

        .widget-about .contact-details {
            margin: 30px 0 0 0;
        }

            .widget-about .contact-details li {
                margin-bottom: 10px;
            }

                .widget-about .contact-details li:last-child {
                    margin-bottom: 0;
                }

                .widget-about .contact-details li span {
                    padding-right: 12px;
                }

                .widget-about .contact-details li a {
                    color: #5867dd;
                }

        @@media (max-width: 991px) {
            .footer-menu {
                padding-left: 0;
            }
        }
    </style>


    <style>
        /* Add this CSS for adjusting styles in phone view */
        @@media (max-width: 767px) {
            .footer-big {
                padding: 30px 15px; /* Adjust padding for smaller screens */
            }

            .footer-widget {
                margin-bottom: 20px; /* Add margin bottom to footer widgets */
            }

            .footer-menu ul {
                padding-left: 0; /* Remove default padding */
            }

            .footer-menu li {
                margin-bottom: 10px; /* Add margin bottom to footer menu items */
            }

                .footer-menu li:last-child {
                    margin-bottom: 0; /* Remove margin bottom for the last item */
                }

                .footer-menu li a {
                    color: #ffffff; /* Set color for footer links */
                }

            .copyright {
                margin-top: 20px; /* Add margin top to copyright */
            }

            .footer--light .copyright {
                color: #ffffff; /* Set color for copyright text */
            }

            .footer-widget .widget-about img {
                margin-bottom: 25px; /* Add margin bottom to the image */
            }
        }
    </style>




    <script>
        function w3_open() {
            document.getElementById("mySidebar").style.display = "block";
        }

        function w3_close() {
            document.getElementById("mySidebar").style.display = "none";
        }
    </script>




</head>
